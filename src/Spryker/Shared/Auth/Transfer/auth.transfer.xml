<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ResetPassword">
        <property name="idResetPassword" type="int"/>
        <property name="fkUserId" type="int"/>
        <property name="code" type="string"/>
        <property name="status" type="string"/>
    </transfer>

    <transfer name="User">
        <property name="password" type="string"/>
        <property name="lastLogin" type="string"/>
        <property name="idUser" type="int"/>
        <property name="firstName" type="string"/>
        <property name="lastName" type="string"/>
        <property name="username" type="string"/>
    </transfer>

    <transfer name="HttpRequest">
        <property name="headers" type="array" singular="header" associative="true"/>
    </transfer>

</transfers>

